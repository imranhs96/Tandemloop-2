n=int(input())
for i in range(1,n+1):
	if i%2!=0:
		a=([j for j in range(1,i+1) if j!=n])
	else:
		a=([j for j in range(i,0,-1) if j!=n])
	a=str(a)
	print(a[1:-1])
for i in range(n-1,1,-1):
	if i%2!=0:
		b=([j for j in range(1,i+1)])
	else:
		b=([j for j in range(i,0,-1)])
	b=str(b)
	print(b[1:-1])
